# .gitignore

# Ignore target directory generated by Maven
/target/

# Ignore IDE files
/.idea/
/.vscode/

# Ignore Node.js modules
/node_modules/

# Ignore build artifacts
/build/

# Ignore environment files
/.env*

# Ignore logs and other generated files
/logs/
*.log

# Ignore generated Gradle files
/build.gradle
/gradle/
/gradlew
/gradlew.bat

# Ignore Eclipse project files
/.classpath
/.project
/.settings/
/.factorypath

# Ignore IntelliJ project files
/*.iml
/.idea/
